    function unpack(t) {
      # assuming tensor t has type tensor<int8>(d0[96])
      expression:tensor<float>(x[768])( unpack_lookup{bit:(x % 8),z:(128 + t{d0:(x / 8)})} )
    }

    function signit()  {
        expression: (2 * unpack(attribute(hash)) -1 )
    }

    function innerproduct() {
        expression: sum(query(query_embedding) * signit())
    }

    first-phase {
      expression: innerproduct()
    }

    second-phase {
      rerank-count: 10
      expression: sum(onnxModel(reader).relevance_logits)
    }

    summary-features {
      onnxModel(reader).start_logits
      onnxModel(reader).end_logits
      input_ids # The input sequence with special tokens (CLS/SEP)
      firstPhase
      rawScore(hash)
    }
  }

  rank-profile sparse inherits openqa {
    first-phase {
      expression: bm25(text) + bm25(title)
    }
  }

  rank-profile dense inherits openqa {
    first-phase {
      expression: innerproduct()
    }
  }

  rank-profile dense-retriever {
      first-phase {
        expression: rawScore(hash) # 1/(1 + hamming distance)
      }
    }
}
